{
  "name": "noteowl-backend",
  "version": "0.0.1",
  "private": true,
  "description": "Note-taking app with calendar",
  "main": "index.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "front": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "go": "concurrently --kill-others \"npm run dev\" \"npm run front\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "mongoose": "^5.5.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-dev": "^2.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
